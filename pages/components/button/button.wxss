/* pages/components/button/button.wxss */
.v-button{
  width: 100%;
  height: 97rpx;
  line-height: 97rpx;
  text-align: center;
  color: #ffffff;
  background: #DB3069;
  border-radius: 100px;
  font-size: 32rpx;
}

.v-button.disabled{
  background: #D8D8D8 !important;
  color: #ffffff !important;
  border:1px solid #D8D8D8 !important;
}

.v-button.v-cycle{
  background: #ffffff;
  border: 3rpx solid #DB3069;
  font-size: 28rpx;
  color: #DB3069;
}

