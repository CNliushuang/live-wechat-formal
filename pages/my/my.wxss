/* pages/my/my.wxss */
.my_wrapper{
  width: 100%;
  height: 100%;
}
.my_wrapper .user{
  border-bottom: 1px solid #E5E5E5;
  padding-bottom: 30rpx;
  padding-top: 20rpx;
}
.my_wrapper .user .icon{
  float: left;
  width: 135rpx;
  height: 135rpx;
  border-radius: 100%;
  background: pink;
  margin: 0 auto;
  margin-left: 40rpx;
  margin-top: 20rpx;
  box-shadow: 0 9px 17px 0 rgba(0,0,0,0.20);
}
.my_wrapper .user .icon image{
  width: 135rpx;
  height: 135rpx;
  border-radius: 100%;
}


.my_wrapper .user .content{
  margin-left: 202rpx;
  font-size: 24rpx;
  color: #9B9B9B;
}
.my_wrapper .user .content view{
  margin-top: 6rpx;
}
.my_wrapper .user .content .nick_name{
  font-size: 32rpx;
  color: #000000;
}

.my_wrapper .user_data{
  border-bottom: 1px solid #E5E5E5;
  padding: 20rpx 33rpx 0 33rpx; 
}
.my_wrapper .user_data .items{
  width: 33%;
  float: left;
  margin-bottom: 23rpx;
}

.my_wrapper .user_data .items .name{
  font-size: 24rpx;
  color: #9B9B9B;
}

.my_wrapper .user_data .items .num{
  font-size: 36rpx;
  color: #DB3069;
  margin-top: 4rpx;
}



.my_wrapper .bind_account{
  font-size: 24rpx;
  color: #9B9B9B;
  padding: 0 33rpx;
  margin-top: 15rpx;
}
.my_wrapper .bind_account .items{
  padding: 15rpx 0;
}
.my_wrapper .bind_account .items .name{
  width: 150rpx;
  float: left;
}
.my_wrapper .bind_account .items .content{
  margin-left: 150rpx;
}

.my_wrapper .bind_account .items .content .go_bind{
  display: inline-block;
  color: #000000;
  margin-left: 10rpx;
}

.my_wrapper .operate{
  padding-top: 92rpx;
}
.my_wrapper .operate .help{
  font-size: 24rpx;
  color: #9E95A1;
  text-align: center;
}

.my_wrapper .operate .items{
  padding: 0 40rpx;
  margin-top: 16rpx;
  margin-bottom: 18rpx;
}

.my_wrapper .operate .protocol{
  padding: 0 40rpx;
  font-size: 24rpx;
  color: #9E95A1;
  height: 33rpx;
  line-height: 33rpx;
  margin-top: 55rpx;
}

.my_wrapper .operate .protocol.help{
  text-align: center;
}

.my_wrapper .operate .protocol .protocol_box{
  width: 410rpx;
  margin: 0 auto;
}

.my_wrapper .operate .protocol v-checkbox{
  float: left;
  margin-right: 10rpx;
}

.my_wrapper .operate .protocol text{
  color: #000000;
}











